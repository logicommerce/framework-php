{%- macro asyncOrder(args) -%}
    {% set spacelessContent %}
        {{ addTimerDebugFlag('basketMacros-asyncOrder',true) }}
        {% set viewParameters = viewHelpers.basket.asyncOrderMacro(args) %}
        {% import 'macros/modes/' ~ coreMode ~ '/basket/asyncOrder.html.twig' as modeBasket %}
        {{ modeBasket.asyncOrder(viewParameters) }}
        {{ addTimerDebugFlag('basketMacros-asyncOrder',false) }}
    {% endset %}
    {{ spacelessContent|replace({'\n': '', '\t': '', '  ': ' '})|trim }}
{%- endmacro asyncOrder -%}

