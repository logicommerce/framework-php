    {% set viewParameters = viewHelpers.user.userFormMacro(args|merge({ routeType: route.type })) %}

    {% from "macros/form.twig" import legalCheck as legalCheckMacro %}
    {% set legalCheck %}
		{{ legalCheckMacro({}) }}
	{% endset %}

    {% import 'macros/modes/' ~ coreMode ~ '/user/userForm.html.twig' as modeUser %}
    {{ modeUser.userForm(viewParameters, legalCheck) }}
    {{ addTimerDebugFlag('userMacros_userForm',false) }}
{% endmacro userForm %}