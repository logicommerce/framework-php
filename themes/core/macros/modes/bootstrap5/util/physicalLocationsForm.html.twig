{%- macro physicalLocationsForm(args) -%}
  <div class="locatorForm selectStoreLocatorForm">
    <div class="selectLocatorTitle storeLocatorTitle">
        {{- languageSheet[constant('FWK\\Enums\\LanguageLabels::PHYSICAL_LOCATION_TITLE')] -}}
    </div>
    {%- set classWildcard = constant('FWK\\Core\\Form\\FormFactory::CLASS_WILDCARD') -%} 
    {%- set attributeWildcard = constant('FWK\\Core\\Form\\FormFactory::ATTRIBUTE_WILDCARD') -%}
    {%- for name, element in args.form.outputElements([constant('FWK\\Enums\\RichFormItems::LC_DATA_VALIDATION')]) -%}
      {%- if name == constant('FWK\\Core\\Form\\Form::ELEMENT_FORM_START') -%}
            {{- element|replace({ (attributeWildcard): 'data-lc-form="selectStoreLocatorForm"' }) -}}
            <div class="form-message"></div>
            <div class="selectStoreLocatorForm">
        {%- elseif name == constant('FWK\\Core\\Form\\Form::SUBMIT') -%}
                {{- legalCheck -}}
                <div class="formField formButton selectStoreLocatorField form-group" id="selectStoreLocator{{- ucwords(name) -}}Container">
                    {{- element|replace({ (classWildcard): '', (attributeWildcard): '' }) -}}
                </div>
                <div class="formResponseContainer" id="selectStoreLocatorResponse"></div>
            </div>
        {%- else -%}
            <div class="form-group formField selectStoreLocatorField" id="selectStoreLocator{{- ucwords(name) -}}Container">
                {{- element -}}
            </div>
        {%- endif -%}
    {%- endfor -%}
{%- endmacro physicalLocationsForm -%}