{%- macro physicalLocationsFilter(args) -%}
    {%- set attributeWildcard = constant('FWK\\Core\\Form\\FormFactory::ATTRIBUTE_WILDCARD') -%}
    {%- set classWildcard = constant('FWK\\Core\\Form\\FormFactory::CLASS_WILDCARD') -%}

    {% set countrySelector = args.countrySelector.element.outputElement(args.countrySelector.name, [constant('FWK\\Enums\\RichFormItems::LC_DATA_VALIDATION')]) %}
    <div class="form-group physicalLocationCountry">
        {{- countrySelector|replace({ 
            (attributeWildcard): 'data-lc-event="change" data-lc-function="getPhysicalLocations"', 
            (classWildcard): '' 
        }) -}}
    </div>

{%- endmacro physicalLocationsFilter -%}